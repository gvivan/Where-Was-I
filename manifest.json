{
  "manifest_version": 3,
  "name": "Where Was I",
  "description": "Helps you pick up exactly where you left off by tracking your bookmarks in the Where Was I folder",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Where Was I - Tracking bookmarks in Where Was I folder"
  },
  "chrome_url_overrides": {
  },
  "web_accessible_resources": [
    {
      "resources": ["welcome.html"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["welcome.js"],
      "matches": ["<all_urls>"]
    }
  ]
} 